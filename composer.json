{
    "name": "aws/aws-sdk-php-silex",
    "homepage": "http://aws.amazon.com/sdkforphp2",
    "description": "A simple Silex service provider for including the AWS SDK for PHP.",
    "keywords": ["silex","aws","amazon","sdk","s3","ec2","dynamodb"],
    "type":"library",
    "license":"Apache-2.0",
    "authors":[
        {
            "name":"Amazon Web Services",
            "homepage":"http://aws.amazon.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "aws/aws-sdk-php": "~3.0",
        "silex/silex": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Aws\\Cilex\\": "src/Cilex",
            "Aws\\Pimple\\": "src/Pimple",
            "Aws\\Silex\\": "src/Silex"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aws\\Cilex\\": "tests/Cilex",
            "Aws\\Pimple\\": "tests/Pimple",
            "Aws\\Silex\\": "tests/Silex"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "require-dev": {
        "pimple/pimple": "~1.0",
        "cilex/cilex": "~1.0",
        "phpunit/phpunit": "~4.7",
        "squizlabs/php_codesniffer": "^2.3"
    }
}
